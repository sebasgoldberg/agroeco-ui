<mvc:View
		height="100%"
		xmlns:core="sap.ui.core"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:semantic="sap.m.semantic"
		displayBlock="true"
        xmlns:l="sap.ui.layout"
        xmlns:f="sap.ui.layout.form"
        controllerName="iamsoft.agroeco.controller.ResolveList">

    <semantic:DetailPage
            title="Resolver Lista"
            showNavButton="true"
		    navButtonPress="onNavBack"
            id="resolveListPage">

        <List
            id="vendorsList"
            items="{vendors>/}"
            selectionChange=""
            mode="MultiSelect"
            includeItemInSelection="true" 
            title="Seleccione los vendedores a utilizar">
            <items>
                <StandardListItem
                    title="{vendors>name}"
                    description=""
                    />
            </items>
        </List>

        <semantic:customFooterContent>
            <Button text="Resolver" type="Emphasized" press="onResolve" id="resolveButton"></Button>
            <Button text="Cancelar" type="Rejects" press="onCancel" id="cancelButton"></Button>
        </semantic:customFooterContent>

    </semantic:DetailPage>

</mvc:View>
