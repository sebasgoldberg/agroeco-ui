<mvc:View
		height="100%"
		xmlns:core="sap.ui.core"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:semantic="sap.m.semantic"
		displayBlock="true"
        controllerName='iamsoft.agroeco.controller.Master'>

        <semantic:MasterPage title="{i18n>master.title}">
			<Toolbar>
				<SearchField
					search="onSearch"
					width="100%"
					text="" />
			</Toolbar>
            <List 
				items="{/}"
				mode="SingleSelectMaster"
				selectionChange="onSelectList"
				>
				<items>
					<ObjectListItem
							title="{name}"
							type="Active"
							press=""
							number="{total}"
							numberUnit="ARS">
						<firstStatus>
							<ObjectStatus
								text="{
									path: 'pending_items',
									formatter: '.formatter.listStatusText'
									}"
								state="{
									path: 'pending_items',
									formatter: '.formatter.listStatusState'
									}" />
						</firstStatus>
						<attributes>
							<ObjectAttribute text="{date}" />
							<!-- <ObjectAttribute text="145 x 140 x 360 cm" /> -->
						</attributes>
						<!-- <markers>
							<ObjectMarker type="Favorite" />
							<ObjectMarker type="Flagged" />
						</markers> -->
					</ObjectListItem>
				</items>
				

            </List>
            <semantic:customFooterContent>
                <Button icon="sap-icon://add" type="Emphasized"
					press="onAddItem"/>
            </semantic:customFooterContent>
        </semantic:MasterPage>

</mvc:View>
