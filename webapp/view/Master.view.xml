<mvc:View
		height="100%"
		xmlns:core="sap.ui.core"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		xmlns:semantic="sap.m.semantic"
		displayBlock="true"
        controllerName='iamsoft.agroeco.controller.Master'>

        <semantic:MasterPage title="{i18n>master.title}">
			<Toolbar>
				<SearchField
					search="onSearch"
					width="100%"
					text="" />
			</Toolbar>
            <List 
				items="{/}"
				mode="SingleSelectMaster"
				selectionChange="onSelectList"
				>
				<StandardListItem
					title="{name}"
					description="{date}"
					icon=""
					iconDensityAware="false"
					iconInset="false" 
					type="Active"
					press=""
					/>
            </List>
            <semantic:customFooterContent>
                <Button icon="sap-icon://add" type="Emphasized"
					press="onAddItem"/>
            </semantic:customFooterContent>
        </semantic:MasterPage>

</mvc:View>
